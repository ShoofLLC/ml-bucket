        }
    },
    "installer" : {
        "args":["--user-install=$dir", "--unattended", "--auto-install=yes"]
    },
    "bin": ["miktex-console.exe", "miktex-texworks.exe"],
    "shortcuts": [
        [
            "miktex-console.exe",
            "miktex-console"
        ],
        [
            "miktex-texworks.exe",
            "miktex-texworks"
        ]
    ],
    "env_add_path": "$dir/MiKTeX/miktex/bin/x64/"
